                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
  select a.productcategory, b.categorydescription, b.datecreated,
	(select count(distinct r.ccastocknumber) from RetailCustomerData r
		join SWCCSSTOK st on r.ccaStockNumber = st.stocknumber and st.productcategory = a.productcategory and 
		r.convertedDate &gt;= '1/1/' + cast(year(getdate()) as varchar(4)) and customerNumber = @a) as 'currentCount',
	(select sum(r.posUnits) from RetailCustomerData r
		join SWCCSSTOK st on r.ccaStockNumber = st.stocknumber and st.productcategory = a.productcategory and 
		r.convertedDate &gt;= '1/1/' + cast(year(getdate()) as varchar(4)) and customerNumber = @a) as 'currentUnitsSold',
	(select sum(r.posAmount) from RetailCustomerData r
		join SWCCSSTOK st on r.ccaStockNumber = st.stocknumber and st.productcategory = a.productcategory and 
		r.convertedDate &gt;= '1/1/' + cast(year(getdate()) as varchar(4)) and customerNumber = @a) as 'currentPosAmount',
		(select count(distinct r.ccastocknumber) from RetailCustomerData r
		join SWCCSSTOK st on r.ccaStockNumber = st.stocknumber and st.productcategory = a.productcategory and 
		r.convertedDate between '1/1/' + cast(year(getdate()) - 1 as varchar(4)) and '1/1/' + cast(year(getdate()) as varchar(4)) and customerNumber = @a) as 'lastCount',
	(select sum(r.posUnits) from RetailCustomerData r
		join SWCCSSTOK st on r.ccaStockNumber = st.stocknumber and st.productcategory = a.productcategory and 
		r.convertedDate between '1/1/' + cast(year(getdate()) - 1 as varchar(4)) and '1/1/' + cast(year(getdate()) as varchar(4)) and customerNumber = @a) as 'lastUnitsSold',
	(select sum(r.posAmount) from RetailCustomerData r
		join SWCCSSTOK st on r.ccaStockNumber = st.stocknumber and st.productcategory = a.productcategory and 
		r.convertedDate between '1/1/' + cast(year(getdate()) - 1 as varchar(4)) and '1/1/' + cast(year(getdate()) as varchar(4)) and customerNumber = @a) as 'lastPosAmount'

  from SWCCSSTOK a
  join SWCCSCATG b on a.productcategory = b.categorycode and b.locationnumber in ('900', '800') and b.datecreated &gt;= '1/1/2014'
  group by b.categorydescription, b.datecreated, a.productcategory


   --select '1/1/' + cast(year(getdate()) - 1 as varchar(4))</CommandText>
